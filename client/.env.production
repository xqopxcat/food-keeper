# 生產環境配置
VITE_APP_ENV=production

# API 成本控制 (生產環境關閉模擬數據)
VITE_USE_MOCK_DATA=false
VITE_VISION_API_DAILY_LIMIT=100
VITE_GEMINI_API_DAILY_LIMIT=50
VITE_OCR_API_DAILY_LIMIT=80

# 調試模式 (生產環境關閉)
VITE_SHOW_DEV_PANEL=false
VITE_ENABLE_API_LOGGING=false

# API Keys (生產環境使用真實 API Keys)
VITE_GOOGLE_VISION_API_KEY=${GOOGLE_VISION_API_KEY}
VITE_GEMINI_API_KEY=${GEMINI_API_KEY}